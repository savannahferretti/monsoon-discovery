{
  "filepaths":{
    "raw":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/raw",
    "interim":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/interim",
    "splits":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/splits",
    "predictions":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/predictions",
    "features":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/features",
    "weights":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/weights",
    "models":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/models"
  },
  "metadata":{
    "author":"Savannah L. Ferretti",
    "email":"savannah.ferretti@uci.edu"
  },
  "domain":{
    "latrange":[5.0,25.0],
    "lonrange":[60.0,90.0],
    "levrange":[500.0,1000.0],
    "years":[2000,2001,2002,2003,2004,2005,2006,2007,2008,2009,2010,2011,2012,2013,2014,2015,2016,2017,2018,2019,2020],
    "months":[6,7,8]
  },
  "splits":{
    "trainyears":[2000,2014],
    "validyears":[2015,2017],
    "testyears":[2018,2020]
  },
  "variables":{
    "field":["bl","cape","subsat","rh","thetae","thetaestar"],
    "local":["lf","lhf","shf"],
    "target":"pr"
  },
  "experiments":{
    "pod":{
      "bins":{"min":-0.6,"max":0.1,"width":0.001,"minsample":50},
      "fit":{"prmin":1.0,"prmax":5.0},
      "landthresh":0.5,
      "runs":{
        "pod_bl":{"withlf":false},
        "pod_bl_lf":{"withlf":true}
      }
    },
    "nn":{
      "projectname":"Chapter-3-Experiments",
      "seeds":[42],
      "workers":8,
      "epochs":20,
      "batchsize":2000,
      "learningrate":0.0005,
      "patience":4,
      "criterion":"MSELoss",
      "runs":{
        "baseline_bl_lf":{"kind":"baseline","fieldvars":["bl"],"localvars":["lf"]},
        "baseline_cape_subsat_lf":{"kind":"baseline","fieldvars":["cape","subsat"],"localvars":["lf"]},
        "baseline_rh_thetae_thetaestar_lf":{"kind":"baseline","fieldvars":["rh","thetae","thetaestar"],"localvars":["lf"]},
        "baseline_rh_thetae_thetaestar_lf_lhf_shf":{"kind":"baseline","fieldvars":["rh","thetae","thetaestar"],"localvars":["lf","hf","shf"]},
        "nonparametric_bl_lf":{"kind":"nonparametric","fieldvars":["bl"],"localvars":["lf"]},
        "gaussian_bl_lf":{"kind":"parametric","fieldvars":["bl"],"localvars":["lf"],"kernel":"gaussian"}
      }
    },
    "sr":{
      "runs":{
        "sr_nonparametric_bl_lf":{"fieldvars":["bl"],"localvars":["lf"],"features_from":"nonparametric_bl_lf"},
        "sr_gaussian_bl_lf":{"fieldvars":["bl"],"localvars":["lf"],"features_from":"gaussian_bl_lf"}
      }
    }
  }
}
