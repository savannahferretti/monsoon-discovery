{
  "filepaths":{
    "raw":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/raw",
    "interim":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/interim",
    "splits":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/splits",
    "predictions":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/predictions",
    "features":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/features",
    "weights":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/data/weights",
    "models":"/global/cfs/cdirs/m4334/sferrett/monsoon-discovery/models"
  },
  "metadata":{
    "author":"Savannah L. Ferretti",
    "email":"savannah.ferretti@uci.edu"
  },
  "domain":{
    "latrange":[5.0,25.0],
    "lonrange":[60.0,90.0],
    "levrange":[500.0,1000.0],
    "years":[2000,2001,2002,2003,2004,2005,2006,2007,2008,2009,2010,2011,2012,2013,2014,2015,2016,2017,2018,2019,2020],
    "months":[6,7,8]
  },
  "splits":{
    "trainyears":[2000,2014],
    "validyears":[2015,2017],
    "testyears":[2018,2020]
  },
  "variables":{
    "field":["bl","cape","subsat","thetaeprime","thetaeplus","t","q"],
    "local":["lf"],
    "target":"pr"
  },
  "experiments":{
    "pod":{
      "bins":{"min":-0.6,"max":0.1,"width":0.001,"minsample":50},
      "fit":{"prmin":1.0,"prmax":5.0},
      "landthresh":0.5,
      "runs":{
        "pod_single_fit":    {"withlf":false},
        "pod_land_ocean_fit":{"withlf":true}
      }
    },
    "nn":{
      "projectname":"Chapter 3 Experiments",
      "seeds":[42,72,102],
      "workers":8,
      "epochs":20,
      "batchsize":500,
      "learningrate":0.0005,
      "patience":4,
      "criterion":"MSELoss",
      "runs":{
        "baseline_experiment_1":{"kind":"baseline","fieldvars":["bl"]},
        "baseline_experiment_2":{"kind":"baseline","fieldvars":["cape","subsat"]},
        "baseline_experiment_3":{"kind":"baseline","fieldvars":["thetaeprime"]},
        "baseline_experiment_4":{"kind":"baseline","fieldvars":["thetaeplus"]},
        "baseline_experiment_5":{"kind":"baseline","fieldvars":["thetaeprime","thetaeplus"]},
        "baseline_experiment_6":{"kind":"baseline","fieldvars":["t","q"]},
        "nonparametric_experiment_1":{"kind":"nonparametric","fieldvars":["bl"]},
        "nonparametric_experiment_2":{"kind":"nonparametric","fieldvars":["cape","subsat"]},
        "nonparametric_experiment_3":{"kind":"nonparametric","fieldvars":["thetaeprime"]},
        "nonparametric_experiment_4":{"kind":"nonparametric","fieldvars":["thetaeplus"]},
        "nonparametric_experiment_5":{"kind":"nonparametric","fieldvars":["thetaeprime","thetaeplus"]},
        "nonparametric_experiment_6":{"kind":"nonparametric","fieldvars":["t","q"]},
        "gaussian_experiment_1":{"kind":"parametric","fieldvars":["bl"],"kernel":"gaussian"},
        "gaussian_experiment_2":{"kind":"parametric","fieldvars":["cape","subsat"],"kernel":"gaussian"},
        "gaussian_experiment_3":{"kind":"parametric","fieldvars":["thetaeprime"],"kernel":"gaussian"},
        "gaussian_experiment_4":{"kind":"parametric","fieldvars":["thetaeplus"],"kernel":"gaussian"},
        "gaussian_experiment_5":{"kind":"parametric","fieldvars":["thetaeprime","thetaeplus"],"kernel":"gaussian"},
        "gaussian_experiment_6":{"kind":"parametric","fieldvars":["t","q"],"kernel":"gaussian"}
      }
    },
    "sr":{
      "runs":{
        "sr_nonparametric_experiment_1":{"fieldvars":["bl"],"features_from":"nonparametric_experiment_1"},
        "sr_nonparametric_experiment_2":{"fieldvars":["cape","subsat"],"features_from":"nonparametric_experiment_2"},
        "sr_nonparametric_experiment_3":{"fieldvars":["thetaeprime"],"features_from":"nonparametric_experiment_3"},
        "sr_nonparametric_experiment_4":{"fieldvars":["thetaeplus"],"features_from":"nonparametric_experiment_4"},
        "sr_nonparametric_experiment_5":{"fieldvars":["thetaeprime","thetaeplus"],"features_from":"nonparametric_experiment_5"},
        "sr_nonparametric_experiment_6":{"fieldvars":["t","q"],"features_from":"nonparametric_experiment_6"},
        "sr_gaussian_experiment_1":{"fieldvars":["bl"],"features_from":"gaussian_experiment_1"},
        "sr_gaussian_experiment_2":{"fieldvars":["cape","subsat"],"features_from":"gaussian_experiment_2"},
        "sr_gaussian_experiment_3":{"fieldvars":["thetaeprime"],"features_from":"gaussian_experiment_3"},
        "sr_gaussian_experiment_4":{"fieldvars":["thetaeplus"],"features_from":"gaussian_experiment_4"},
        "sr_gaussian_experiment_5":{"fieldvars":["thetaeprime","thetaeplus"],"features_from":"gaussian_experiment_5"},
        "sr_gaussian_experiment_6":{"fieldvars":["t","q"],"features_from":"gaussian_experiment_6"}
      }
    }
  }
}
